
public class Test {
	
	public static void main(String[] args) {
		double t = 0, f = 0;
		for (int i = 0; i < 1000; i++) { // 1000 repetitions
			if (Math.max(Math.max(Math.random(), Math.random()), Math.max(Math.random(), Math.random())) > 0.07) t++;
			else f++;
		} System.out.println(t / (t + f));
	}
	
}

