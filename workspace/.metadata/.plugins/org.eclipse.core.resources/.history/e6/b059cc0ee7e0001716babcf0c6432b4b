import java.util.*;

public class Crosswalk {
	
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		boolean[] crosswalks = new boolean[scanner.nextInt()];
		int groupSize = scanner.nextInt(), numBroken = scanner.nextInt();
		for (int i = 0; i < numBroken; i++) crosswalks[scanner.nextInt()] = true;
		System.out.println(Arrays.toString(crosswalks));
	}

}

/*
10 6 5
9
2
10
5
1

Output: 1
*/