
public class Test {
	
	public static void main(String[] args) {
		double t = 0, f = 0;
		trials: for (int i = 0; i < 1000000; i++) { // 1 million repetitions
			for (int j = 0; j < 19; j++) {
				if (Math.random() < 0.07) f++;
				continue trials;
			} t++;
		} System.out.println(t / (t + f));
	}
	
}

